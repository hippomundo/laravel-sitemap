{
    "name": "hippomundo/sitemap",
    "description": "A maintained fork of the Laravelium Sitemap package for Laravel.",
    "homepage": "https://www.hippomundo.com",
    "keywords": ["laravelium", "laravel", "php", "sitemap", "generator", "xml", "html", "google-news"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rumen Damyanov",
            "email": "r@alfamatter.com",
            "role": "Developer",
            "homepage": "https://darumen.com"
        },
        {
            "name": "Koen Calliauw",
            "email": "koen.calliauw@hippomundo.com",
            "role": "Developer",
            "homepage": "https://www.hippomundo.com"
        }
    ],
    "support": {
        "issues": "https://github.com/hippomundo/laravel-sitemap/issues",
        "source": "https://github.com/hippomundo/laravel-sitemap",
        "wiki": "https://github.com/hippomundo/laravel-sitemap/wiki"
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "illuminate/filesystem": "^10.0"
    },
    "require-dev": {
        "laravel/framework": "^10.0",
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench-core": "^8.0",
        "php-coveralls/php-coveralls" : "^2.5"
    },
    "autoload": {
        "psr-0": {
            "Laravelium\\Sitemap": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravelium\\Sitemap\\SitemapServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
